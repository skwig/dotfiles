{
    "height": 30,
    "margin": 8,
    "spacing": 4,
    "modules-left": [
        "custom/distrologo",
        "custom/media",
        "hyprland/workspaces",
        "tray",
        "hyprland/window"
    ],
    "modules-center": [],
